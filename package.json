{
  "name": "all-valid-words",
  "version": "1.0.0",
  "main": "src/handler.js",
  "license": "MIT",
  "scripts": {
    "deploy": "sls deploy --stage production",
    "lint": "eslint --ext .js,.ts ./src",
    "lint:types": "tsc --noEmit",
    "tail:hello": "serverless logs --function hello --tail",
    "test": "jest --watch",
    "watch:hello": "serverless invoke local --watch --function hello --path fixtures/event.json",
    "watch:warm": "serverless invoke local --watch --function hello --path fixtures/scheduled.json",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@tsconfig/node18": "2.0.1",
    "@types/aws-lambda": "8.10.115",
    "@types/memoizee": "0.4.8",
    "@types/node": "18.16.16",
    "@types/wordfilter": "0.2.0",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "esbuild": "0.17.19",
    "eslint": "8.41.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "serverless": "3.31.0",
    "serverless-esbuild": "1.45.0",
    "typescript": "5.0.4"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.341.0",
    "@aws-sdk/client-s3": "3.341.0",
    "@aws-sdk/client-secrets-manager": "3.341.0",
    "@aws-sdk/client-sqs": "3.341.0",
    "@aws-sdk/util-dynamodb": "3.341.0",
    "@js-joda/core": "5.5.3",
    "memoizee": "0.4.15",
    "tslib": "2.5.2",
    "wordfilter": "0.2.6"
  }
}
